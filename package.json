{
  "name": "ngx-i18n-country-select",
  "version": "1.1.0",
  "description": "Angular4 Bootstrap4 i18n country select input",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ngx-i18n-country-select -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "test": "tsc && karma start",
    "clean": "rm *.js src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/osahner/ngx-i18n-country-select.git"
  },
  "author": {
    "name": "Oliver Sahner",
    "email": "osahner@medianet.de"
  },
  "keywords": [
    "angular",
    "bootstrap",
    "password"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/osahner/ngx-i18n-country-select.git/issues"
  },
  "devDependencies": {
    "@angular/common": "^4.3.4",
    "@angular/compiler": "^4.3.4",
    "@angular/compiler-cli": "^4.3.4",
    "@angular/core": "^4.3.4",
    "@angular/forms": "^4.3.4",
    "@angular/platform-browser": "^4.3.4",
    "@angular/platform-browser-dynamic": "^4.3.4",
    "@compodoc/compodoc": "^1.0.0-beta.14",
    "@types/jasmine": "2.5.53",
    "@types/node": "^8.0.22",
    "codelyzer": "~3.1.2",
    "core-js": "^2.5.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.14.0",
    "jasmine-core": "~2.6.4",
    "jasmine-spec-reporter": "~4.1.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-typescript": "^3.0.5",
    "karma-typescript-angular2-transform": "^1.0.1",
